{
  "name": "mapbox-use",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@8.15.9",
  "description": "",
  "author": "Chizuki <chizukicn@outlook.com>",
  "license": "MIT",
  "repository": "chizukicn/mapbox-use",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./handlers": {
      "types": "./dist/handlers.d.ts",
      "require": "./dist/handlers.cjs",
      "import": "./dist/handlers.mjs"
    },
    "./test-utils": {
      "types": "./dist/test-utils.d.ts",
      "require": "./dist/test-utils.cjs",
      "import": "./dist/test-utils.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev": "vitest dev",
    "lint": "eslint --cache -f mo --ext .ts,.js,.mjs,.cjs .",
    "lint:fix": "pnpm lint --fix",
    "prepare": "npx simple-git-hooks",
    "prepack": "pnpm run build",
    "release": "changelogen --release &&  pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org",
    "prepublish": "pnpm run build",
    "test": "vitest",
    "up:deps": "taze major",
    "up:polyfill": "nolyfill install"
  },
  "peerDependencies": {
    "mapbox-gl": "^3.0.0 || ^2.0.0",
    "vue": "^3.5.12"
  },
  "dependencies": {
    "maybe-types": "^0.2.0"
  },
  "devDependencies": {
    "@curev/eslint-config": "^0.3.7",
    "@types/mapbox-gl": "^2.7.21",
    "@vitest/coverage-v8": "^1.6.0",
    "changelogen": "^0.5.7",
    "eslint": "^8.57.1",
    "eslint-formatter-mo": "^1.2.0",
    "lint-staged": "^15.2.10",
    "nolyfill": "^1.0.42",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  },
  "pnpm": {
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@latest",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@latest",
      "has": "npm:@nolyfill/has@latest",
      "object.values": "npm:@nolyfill/object.values@latest"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs}": [
      "eslint --cache --fix"
    ]
  }
}
